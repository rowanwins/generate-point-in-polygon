{
  "name": "random-point-in-polygon",
  "version": "0.0.3",
  "description": "A module to generate a random point in a polygon",
  "main": "dist/randomPointInPolygon.js",
  "module": "src/main.js",
  "unpkg": "dist/randomPointInPolygon.min.js",
  "jsdelivr": "dist/randomPointInPolygon.min.js",
  "scripts": {
    "bench": "npm run build && node test/benchmark.js",
    "build": "rollup -c",
    "debug": "cross-env webpack-dev-server --config debug/webpack.config.js --mode development --open --hot",
    "test": "ava --verbose",
    "test:e2e": "ava ./test/test.spec.js --verbose",
    "test:coverage": "nyc npm run test"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "is-clockwise": "^2.0.2",
    "point-in-polygon": "^1.0.1"
  },
  "devDependencies": {
    "@turf/point-on-feature": "^5.1.5",
    "ava": "^1.0.1",
    "benchmark": "^2.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-mourner": "^3.0.0",
    "esm": "^3.0.84",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "json-loader": "^0.5.7",
    "leaflet": "^1.4.0",
    "load-json-file": "^5.1.0",
    "nyc": "^13.1.0",
    "rollup": "^1.16.2",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
